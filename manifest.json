{
    "manifest_version": 2,
    "name": "Forex Trend Helper",
    "version": "1.6",
    "permissions": [
         "https://fx-trend.com/*"
    ],
    "content_scripts": [ {
        "js": [ "var/js/jquery.js", "var/js/javascript.js" ],
        "matches": [ "https://fx-trend.com/*"]
    }],
    "browser_action": {
        "name": "Manipulate DOM",
        "icons": ["var/img/favicon.png"],
        "default_icon": "var/img/favicon.png"
    }
}